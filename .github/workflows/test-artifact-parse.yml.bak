on:
  workflow_dispatch:
    inputs:
      build_run_id:
        description: "Run ID of build workflow"
        required: true

jobs:
  test:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/download-artifact@v4
        with:
          run-id: ${{ inputs.build_run_id }}
          path: artifacts
          merge-multiple: true

      - run: ls -R artifacts
